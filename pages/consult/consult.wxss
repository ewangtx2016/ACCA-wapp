/* pages/consult/consult.wxss */
.container {
  height: 100vh;
  background: #eaeaea;
  overflow: hidden;
}
.fuckme {
  width: 94%;
  box-sizing: border-box;
  font-size: 28rpx;
  margin: 30rpx auto 0rpx;
  padding: 15rpx 20rpx;
  line-height: 40rpx;
  border-radius: 10rpx;
  background: #fff;
}
.phone {
  display: flex;
  margin-top: 30rpx;
  padding: 16rpx 3%;
  line-height: 52rpx;
  background: #fff;
}
.phone-num {
  font-size: 28rpx;
}
.phone-num text {
  color: #aaaaaa;
}
.phone-input {
  flex: 1;
  padding-left: 20rpx;
}
.btn {
  width: 94%;
  height: 80rpx;
  line-height: 80rpx;
  margin: 30rpx auto 0;
  font-size: 32rpx;
  text-align: center;
  border-radius: 10rpx;
  background: red;
  color: #fff;
}